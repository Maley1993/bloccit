<h1>All Questions</h1>
<button type="button" class="btn btn-success"><%= link_to "New Question", new_question_path %></button>
<% @questions.each do |question| %>
  <div class="media">
   <div class="media-body">
     <h4 class="media-heading">
       <%= link_to question.title, question %>
     </h4>
   </div>
  </div>
<% end %>
